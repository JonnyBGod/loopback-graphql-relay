{
  "name": "Book",
  "base": "PersistedModel",
  "idInjection": true,
  "properties": {
    "name": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "linkedPeople": {
      "type": "embedsMany",
      "model": "Link",
      "scope": {
        "include": "linked"
      }
    },
    "authors": {
      "type": "hasAndBelongsToMany",
      "model": "Author",
      "foreignKey": ""
    },
    "sites": {
      "type": "hasAndBelongsToMany",
      "model": "Site",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
